{
  "name": "clic-clac",
  "version": "2.6.0",
  "description": "Accessible tabs and toggle buttons",
  "main": "lib/clic-clac.js",
  "scripts": {
    "changelog": "gren changelog --generate --override",
    "test": "eslint ./src && jest",
    "dev": "webpack-dev-server --mode development --content-base dev --inline --hot",
    "start": "typedoc --excludeNotExported --ignoreCompilerErrors --module es6 --out ./doc ./src/js/clic-clac.ts && webpack --mode production"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/jest": "22.2.0",
    "babel-loader": "8.0.4",
    "eslint": "4.9.0",
    "github-release-notes": "0.16.0",
    "jest": "22.4.2",
    "jest-puppeteer-preset": "2.0.0",
    "prettier": "1.11.1",
    "puppeteer": "1.1.1",
    "tslint": "5.9.1",
    "tslint-config-prettier": "1.9.0",
    "tslint-plugin-prettier": "1.3.0",
    "typedoc": "0.13.0",
    "typescript": "3.1.6",
    "uglifyjs-webpack-plugin": "1.2.2",
    "webpack": "4.0.1",
    "webpack-cli": "2.0.9",
    "webpack-dev-server": "3.1.10"
  },
  "jest": {
    "preset": "jest-puppeteer-preset",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "roots": [
      "integration-tests"
    ],
    "transform": {
      ".+\\.(ts|tsx)$": "./integration-tests/preprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "core-js": "2.5.3",
    "custom-event": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mynameislau/clic-clac.git"
  },
  "keywords": [
    "collapse",
    "aria",
    "tablist"
  ],
  "bugs": {
    "url": "https://github.com/mynameislau/clic-clac/issues"
  },
  "homepage": "https://github.com/mynameislau/clic-clac#readme"
}
